{
    "$schema": "https://biomejs.dev/schemas/1.5.3/schema.json",
    "organizeImports": {
        "enabled": false
    },
    "formatter": {
        "enabled": true,
        "indentSize": 2,
        "indentStyle": "space",
        "lineWidth": 120
    },
    "linter": {
        "enabled": true,
        "rules": {
            "recommended": true,
            "complexity": {
                "useLiteralKeys": "off"
            }
        }
    }
}
